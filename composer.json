{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "doctrine/doctrine-bundle": "^2.15",
    "doctrine/doctrine-migrations-bundle": "^3.4.2",
    "doctrine/orm": "^3.5.0",
    "gedmo/doctrine-extensions": "^3.13",
    "illuminate/contracts": "^v12",
    "illuminate/support": "^v12",
    "illuminate/validation": "^v12",
    "jucksearm/php-barcode": "^1.0",
    "kreait/firebase-bundle": "^5.6",
    "kreait/firebase-php": "^7.18.0",
    "lexik/jwt-authentication-bundle": "^v3.1.1",
    "nelmio/api-doc-bundle": "^v5.6.3",
    "nelmio/cors-bundle": "^2.5",
    "nesbot/carbon": "^3.9.1",
    "phpdocumentor/reflection-docblock": "^5.3",
    "phpoffice/phpspreadsheet": "^3.3",
    "phpstan/phpdoc-parser": "^1.24",
    "stof/doctrine-extensions-bundle": "^1.9",
    "symfony/asset": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/doctrine-messenger": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/monolog-bundle": "^3.10",
    "symfony/password-hasher": "7.3.*",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/scheduler": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/twig-bundle": "7.3.*",
    "symfony/uid": "7.3.*",
    "symfony/yaml": "7.3.*",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^12.3",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "7.3.*",
    "symfony/css-selector": "7.3.*",
    "symfony/maker-bundle": "^1.51"
  }
}
